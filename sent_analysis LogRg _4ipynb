    "def sentiment_score(text):\n",
    "    tokens=tokenizer.encode(text,return_tensors=\"pt\")\n",
    "    result=model(tokens)\n",
    "    return int(torch.argmax(result.logits))+1\n",
    "\n",
    "\n"
   ]
